{
  "name": "@firelayer/cli",
  "version": "1.0.0-alpha.4",
  "description": "Firelayer's CLI - Jumpstart your Firebase Web Project",
  "keywords": [
    "firelayer",
    "generator",
    "boilerplate",
    "firebase",
    "cli"
  ],
  "homepage": "https://github.com/firelayer/firelayer/tree/master/lib/cli",
  "bugs": {
    "url": "https://github.com/firelayer/firelayer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/firelayer/firelayer.git",
    "directory": "lib/cli"
  },
  "license": "MIT",
  "author": "Jo√£o Teixeira <joao@indielayer.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "firelayer": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@firelayer/core": "^1.0.0-alpha.4",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^3.0.0",
    "cross-spawn": "^7.0.2",
    "envinfo": "^7.5.0",
    "find-up": "^4.1.0",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "ignore": "^5.1.4",
    "inquirer": "^7.1.0",
    "listr": "^0.14.3",
    "semver": "^7.1.3",
    "shelljs": "^0.8.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "yarn clean && tsc -w --preserveWatchOutput",
    "manifest": "oclif-dev manifest",
    "docs": "yarn manifest && oclif-dev readme --multi",
    "version": "yarn docs && git add ."
  },
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "firelayer",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
